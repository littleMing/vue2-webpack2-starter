{
  "name": "vue2-webpack2-starter",
  "version": "1.0.0",
  "description": "vue2,webpack2项目",
  "private": true,
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --quiet --port 5001 --host 0.0.0.0",
    "build": "node ./node_modules/webpack/bin/webpack.js --progress --hide-modules",
    "prod": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --progress --display-modules"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.1",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-runtime": "^5.8.0",
    "css-loader": "^0.21.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.5",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-manifest-plugin": "^0.5.0",
    "yargs": "^3.31.0"
  },
  "dependencies": {}
}
